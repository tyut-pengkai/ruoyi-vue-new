<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.license.mapper.SysLicenseRecordMapper">

    <resultMap type="SysLicenseRecord" id="SysLicenseRecordResult">
        <result property="id" column="id"/>
        <result property="webUrl" column="web_url"/>
        <result property="deviceCode" column="device_code"/>
        <result property="name" column="name"/>
        <result property="contact" column="contact"/>
        <result property="status" column="status"/>
        <result property="startTime" column="start_time"/>
        <result property="endTime" column="end_time"/>
        <result property="loginCode" column="login_code"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="remark" column="remark"/>
    </resultMap>

    <sql id="selectSysLicenseRecordVo">
        select id,
               web_url,
               device_code,
               name,
               contact,
               status,
               start_time,
               end_time,
               login_code,
               create_by,
               create_time,
               update_by,
               update_time,
               remark
        from sys_license_record
    </sql>

    <select id="selectSysLicenseRecordList" parameterType="SysLicenseRecord" resultMap="SysLicenseRecordResult">
        <include refid="selectSysLicenseRecordVo"/>
        <where>
            <if test="webUrl != null  and webUrl != ''">and web_url = #{webUrl}</if>
            <if test="deviceCode != null  and deviceCode != ''">and device_code like concat('%', #{deviceCode}, '%')
            </if>
            <if test="name != null  and name != ''">and name like concat('%', #{name}, '%')</if>
            <if test="contact != null  and contact != ''">and contact like concat('%', #{contact}, '%')</if>
            <if test="status != null  and status != ''">and status like concat('%', #{status}, '%')</if>
            <if test="params.beginStartTime != null and params.beginStartTime != '' and params.endStartTime != null and params.endStartTime != ''">
                and start_time between #{params.beginStartTime} and #{params.endStartTime}
            </if>
            <if test="params.beginEndTime != null and params.beginEndTime != '' and params.endEndTime != null and params.endEndTime != ''">
                and end_time between #{params.beginEndTime} and #{params.endEndTime}
            </if>
            <if test="loginCode != null  and loginCode != ''">and login_code like concat('%', #{loginCode}, '%')</if>
        </where>
    </select>

    <select id="selectSysLicenseRecordById" parameterType="Long" resultMap="SysLicenseRecordResult">
        <include refid="selectSysLicenseRecordVo"/>
        where id = #{id}
    </select>

    <insert id="insertSysLicenseRecord" parameterType="SysLicenseRecord">
        insert into sys_license_record
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="webUrl != null">web_url,</if>
            <if test="deviceCode != null">device_code,</if>
            <if test="name != null">name,</if>
            <if test="contact != null">contact,</if>
            <if test="status != null">status,</if>
            <if test="startTime != null">start_time,</if>
            <if test="endTime != null">end_time,</if>
            <if test="loginCode != null">login_code,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="webUrl != null">#{webUrl},</if>
            <if test="deviceCode != null">#{deviceCode},</if>
            <if test="name != null">#{name},</if>
            <if test="contact != null">#{contact},</if>
            <if test="status != null">#{status},</if>
            <if test="startTime != null">#{startTime},</if>
            <if test="endTime != null">#{endTime},</if>
            <if test="loginCode != null">#{loginCode},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
        </trim>
    </insert>

    <update id="updateSysLicenseRecord" parameterType="SysLicenseRecord">
        update sys_license_record
        <trim prefix="SET" suffixOverrides=",">
            <if test="webUrl != null">web_url = #{webUrl},</if>
            <if test="deviceCode != null">device_code = #{deviceCode},</if>
            <if test="name != null">name = #{name},</if>
            <if test="contact != null">contact = #{contact},</if>
            <if test="status != null">status = #{status},</if>
            <if test="startTime != null">start_time = #{startTime},</if>
            <if test="endTime != null">end_time = #{endTime},</if>
            <if test="loginCode != null">login_code = #{loginCode},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteSysLicenseRecordById" parameterType="Long">
        delete
        from sys_license_record
        where id = #{id}
    </delete>

    <delete id="deleteSysLicenseRecordByIds" parameterType="String">
        delete from sys_license_record where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>