    <select id="getBeContinuedNumber" resultType="java.lang.Integer">
        SELECT
            COUNT(1)
        FROM
            wms_sale_return_order_detail AS wccrod
                LEFT JOIN wms_ret_task AS wrt ON wccrod.id = wrt.detail_id
                LEFT JOIN wms_sale_return_order AS wccro ON wccro.order_no = wccrod.return_order_no
        WHERE
            wrt.task_type = #{taskType}
          AND wrt.task_status IN ('1','2') AND wccro.order_no = #{orderNo}
    </select>

