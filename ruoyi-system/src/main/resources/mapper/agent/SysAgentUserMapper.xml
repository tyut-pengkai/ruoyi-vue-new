    <sql id="selectSysAgentVo">
        select ag.agent_id,
               ag.parent_agent_id,
               ag.path,
               ag.user_id,
               ag.enable_add_subagent,
               ag.expire_time,
               ag.status,
               ag.del_flag,
               ag.create_by,
               ag.create_time,
               ag.update_by,
               ag.update_time,
               ag.remark,
               ag.enable_update_subagent_password, ag.enable_update_subagent_status, ag.enable_unbind_app_user, ag.enable_update_app_user_status_0, ag.enable_update_app_user_status_1,
               ag.enable_update_app_user_time, ag.enable_update_app_user_point, ag.enable_update_app_user_login_limit_u, ag.enable_update_app_user_login_limit_m, ag.enable_update_app_user_custom_params,
               ag.enable_update_app_user_remark, ag.enable_view_user_contact, ag.enable_update_user_contact, ag.enable_update_user_password, ag.enable_update_user_remark, ag.enable_update_card_time,
               ag.enable_update_card_point, ag.enable_update_card_login_limit_u, ag.enable_update_card_login_limit_m, ag.enable_update_card_custom_params, ag.enable_update_card_remark,
               ag.enable_update_card_status_0, ag.enable_update_card_status_1, ag.enable_batch_card_replace, ag.enable_add_card, ag.enable_delete_subagent, ag.enable_delete_app_user, ag.enable_delete_card,
               u.user_id,
               u.user_name,
               u.nick_name
        from sys_agent ag
                 left join sys_user u on ag.user_id = u.user_id
    </sql>

    <select id="selectSysAgentList" parameterType="SysAgent" resultMap="SysAgentResult">
        <include refid="selectSysAgentVo"/>
        where ag.del_flag = '0'
        <if test="parentAgentId != null ">and ag.parent_agent_id = #{parentAgentId}</if>
        <if test="params.userName != null ">and (u.user_name like concat('%', #{params.userName}, '%') or
            u.nick_name like concat('%', #{params.userName}, '%'))
        </if>
        <if test="enableAddSubagent != null  and enableAddSubagent != ''">and ag.enable_add_subagent =
            #{enableAddSubagent}
        </if>
        <if test="params.beginExpireTime != null and params.beginExpireTime != '' and params.endExpireTime != null and params.endExpireTime != ''">
            and ag.expire_time between #{params.beginExpireTime} and #{params.endExpireTime}
        </if>
        <if test="status != null  and status != ''">and ag.status = #{status}</if>
        <if test="remark != null and remark != ''">and ag.remark like concat('%', #{remark}, '%')</if>
        <if test="enableUpdateSubagentPassword != null  and enableUpdateSubagentPassword != ''"> and enable_update_subagent_password = #{enableUpdateSubagentPassword}</if>
        <if test="enableUpdateSubagentStatus != null  and enableUpdateSubagentStatus != ''"> and enable_update_subagent_status = #{enableUpdateSubagentStatus}</if>
        <if test="enableUnbindAppUser != null  and enableUnbindAppUser != ''"> and enable_unbind_app_user = #{enableUnbindAppUser}</if>
