<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.OaProjectStageMapper">
    
    <resultMap type="OaProjectStage" id="OaProjectStageResult">
        <result property="id"    column="id"    />
        <result property="projectId"    column="project_id"    />
        <result property="stageType"    column="stage_type"    />
        <result property="stageName"    column="stage_name"    />
        <result property="startDate"    column="start_date"    />
        <result property="endDate"    column="end_date"    />
        <result property="status"    column="status"    />
        <result property="attachmentUrl"    column="attachment_url"    />
        <result property="createdBy"    column="created_by"    />
        <result property="createdTime"    column="created_time"    />
        <result property="updatedBy"    column="updated_by"    />
        <result property="updatedTime"    column="updated_time"    />
    </resultMap>

    <sql id="selectOaProjectStageVo">
        select id, project_id, stage_type, stage_name, start_date, end_date, status, attachment_url, created_by, created_time, updated_by, updated_time from oa_project_stage
    </sql>

    <select id="selectOaProjectStageList" parameterType="OaProjectStage" resultMap="OaProjectStageResult">
        <include refid="selectOaProjectStageVo"/>
        <where>  
            <if test="projectId != null "> and project_id = #{projectId}</if>
            <if test="stageType != null "> and stage_type = #{stageType}</if>
            <if test="stageName != null  and stageName != ''"> and stage_name like concat('%', #{stageName}, '%')</if>
            <if test="startDate != null "> and start_date = #{startDate}</if>
            <if test="endDate != null "> and end_date = #{endDate}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="attachmentUrl != null  and attachmentUrl != ''"> and attachment_url = #{attachmentUrl}</if>
            <if test="createdBy != null "> and created_by = #{createdBy}</if>
            <if test="createdTime != null "> and created_time = #{createdTime}</if>
            <if test="updatedBy != null "> and updated_by = #{updatedBy}</if>
            <if test="updatedTime != null "> and updated_time = #{updatedTime}</if>
        </where>
    </select>
    
    <select id="selectOaProjectStageById" parameterType="Long" resultMap="OaProjectStageResult">
        <include refid="selectOaProjectStageVo"/>
        where id = #{id}
    </select>

    <insert id="insertOaProjectStage" parameterType="OaProjectStage" useGeneratedKeys="true" keyProperty="id">
        insert into oa_project_stage
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="projectId != null">project_id,</if>
            <if test="stageType != null">stage_type,</if>
            <if test="stageName != null and stageName != ''">stage_name,</if>
            <if test="startDate != null">start_date,</if>
            <if test="endDate != null">end_date,</if>
            <if test="status != null">status,</if>
            <if test="attachmentUrl != null">attachment_url,</if>
            <if test="createdBy != null">created_by,</if>
            <if test="createdTime != null">created_time,</if>
            <if test="updatedBy != null">updated_by,</if>
            <if test="updatedTime != null">updated_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="projectId != null">#{projectId},</if>
            <if test="stageType != null">#{stageType},</if>
            <if test="stageName != null and stageName != ''">#{stageName},</if>
            <if test="startDate != null">#{startDate},</if>
            <if test="endDate != null">#{endDate},</if>
            <if test="status != null">#{status},</if>
            <if test="attachmentUrl != null">#{attachmentUrl},</if>
            <if test="createdBy != null">#{createdBy},</if>
            <if test="createdTime != null">#{createdTime},</if>
            <if test="updatedBy != null">#{updatedBy},</if>
            <if test="updatedTime != null">#{updatedTime},</if>
         </trim>
    </insert>

    <update id="updateOaProjectStage" parameterType="OaProjectStage">
        update oa_project_stage
        <trim prefix="SET" suffixOverrides=",">
            <if test="projectId != null">project_id = #{projectId},</if>
            <if test="stageType != null">stage_type = #{stageType},</if>
            <if test="stageName != null and stageName != ''">stage_name = #{stageName},</if>
            <if test="startDate != null">start_date = #{startDate},</if>
            <if test="endDate != null">end_date = #{endDate},</if>
            <if test="status != null">status = #{status},</if>
            <if test="attachmentUrl != null">attachment_url = #{attachmentUrl},</if>
            <if test="createdBy != null">created_by = #{createdBy},</if>
            <if test="createdTime != null">created_time = #{createdTime},</if>
            <if test="updatedBy != null">updated_by = #{updatedBy},</if>
            <if test="updatedTime != null">updated_time = #{updatedTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteOaProjectStageById" parameterType="Long">
        delete from oa_project_stage where id = #{id}
    </delete>

    <delete id="deleteOaProjectStageByIds" parameterType="String">
        delete from oa_project_stage where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>