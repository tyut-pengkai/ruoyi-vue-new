        tenant_id,
        create_by,
        create_time,
        dept_id,
        update_by,
        update_time,
        container_no,
        container_type,
        )
        VALUES
        <foreach item="item" collection="list" separator=",">
            (#{item.orderNo},
            #{item.factoryCode},
            #{item.factoryName},
            #{item.saleCode},
            #{item.saleDesc},
            #{item.moveType},
            #{item.saleMgType},
            #{item.customerCode},
            #{item.innerOrderNo},
            #{item.orderStatus},
