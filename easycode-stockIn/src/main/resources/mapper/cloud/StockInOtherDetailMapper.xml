    <select id="stockInOtherOrderDetailList" resultMap="StockinOtherDetailDto">
        SELECT
        wsod.id,
        wsod.material_no,
        wsod.stge_loc,
        wti.task_no,
        wsod.line_no,
        wti.stockin_order_no,
        wsod.material_no,
        wti.position_no,
        wsod.unit,
        wsod.move_type,
        wsod.lot,
        wsod.qty
        FROM wms_stockin_other_detail as wsod
        left join wms_ret_task wti on wsod.id=wti.detail_id
        <where>
            <if test="id !=null and id!= ''">
                AND wti.id = #{id}
            </if>
        </where>
    </select>

    <select id="stockInOtherOrderDetailListByTaskNo" resultMap="StockinOtherDetailDto">
        SELECT
        wti.id,
        wsod.material_no,
        wsod.stge_loc,
        wti.task_no,
        wsod.line_no,
        wti.stockin_order_no,
