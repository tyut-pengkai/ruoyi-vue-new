        </where>
    </select>

    <select id="selectStockInOtherDetailList" parameterType="StockInOtherDetail" resultMap="StockInOtherDetailResult">
        <include refid="selectStockInOtherDetailVo"/>
        <where>
            <if test="orderNo != null  and orderNo != ''"> and order_no = #{orderNo}</if>
            <if test="lineNo != null  and lineNo != ''"> and line_no = #{lineNo}</if>
            <if test="materialNo != null  and materialNo != ''"> and material_no = #{materialNo}</if>
            <if test="materialName != null  and materialName != ''"> and material_name like concat('%', #{materialName}, '%')</if>
            <if test="oldMaterialNo != null  and oldMaterialNo != ''"> and old_material_no = #{oldMaterialNo}</if>
            <if test="qty != null "> and qty = #{qty}</if>
            <if test="unit != null  and unit != ''"> and unit = #{unit}</if>
            <if test="lot != null  and lot != ''"> and lot = #{lot}</if>
            <if test="isConsigned != null  and isConsigned != ''"> and is_consigned = #{isConsigned}</if>
            <if test="isQc != null  and isQc != ''"> and is_qc = #{isQc}</if>
            <if test="factoryCode != null  and factoryCode != ''"> and factory_code = #{factoryCode}</if>
            <if test="supplierCode != null  and supplierCode != ''"> and supplier_code = #{supplierCode}</if>
            <if test="supplierId != null "> and supplier_id = #{supplierId}</if>
            <if test="tenantId != null "> and tenant_id = #{tenantId}</if>
            <if test="containerNo != null "> and container_no = #{containerNo}</if>
            <if test="containerType != null "> and container_type = #{containerType}</if>
            <if test="wbsElement != null">and wbs_element = #{wbsElement}</if>
            <if test="isProject != null">and is_project = #{isProject}</if>
        </where>
    </select>

    <select id="getPrintInfoByIds" resultMap="PrintInfoResult">
        SELECT
        cso.order_no purchase_order_no,
        csod.material_no,
        csod.material_name,
        csod.old_material_no,
        csod.qty deliver_qty,
        csod.lot lot_no
        FROM
        wms_stockin_other_detail csod
        LEFT JOIN wms_stockin_other cso ON cso.order_no = csod.order_no
        where cso.id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </select>

    <select id="selectStockInOtherDetailById" parameterType="Long" resultMap="StockInOtherDetailResult">
        <include refid="selectStockInOtherDetailVo"/>
        where id = #{id}
    </select>
    <select id="getBeContinuedNumber" resultType="java.lang.Integer">
        SELECT
            COUNT( 1 )
        FROM
            wms_stockin_other_detail AS wsod
                LEFT JOIN wms_ret_task AS wrt ON wsod.id = wrt.detail_id
                LEFT JOIN wms_stockin_other AS wso ON wso.order_no = wsod.order_no
        WHERE
            wrt.task_type = #{taskType}
          AND wrt.task_status IN ('1','2') AND wso.order_no = #{orderNo}
    </select>
    <select id="queryStockInOtherDetailByTaskNo"
            resultType="com.easycode.cloud.domain.vo.StockInDetailPrintVo">
        SELECT
        wsod.material_no as materialNo,
        wsod.factory_code as factoryCode,
        wsod.stge_loc as locationCode,
        wti.task_no as taskNo,
        wti.stockin_order_no as stockinOrderNo,
