        <if test="enableUpdateAppUserTime != null  and enableUpdateAppUserTime != ''"> and enable_update_app_user_time = #{enableUpdateAppUserTime}</if>
        <if test="enableUpdateAppUserPoint != null  and enableUpdateAppUserPoint != ''"> and enable_update_app_user_point = #{enableUpdateAppUserPoint}</if>
