<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.SysAppTrialUserMapper">

    <resultMap type="SysAppTrialUser" id="SysAppTrialUserResult">
        <result property="appTrialUserId" column="app_trial_user_id"/>
        <result property="appId" column="app_id"/>
        <result property="status" column="status"/>
        <result property="lastLoginTime" column="last_login_time"/>
        <result property="loginTimes" column="login_times"/>
        <result property="loginTimesAll" column="login_times_all"/>
        <result property="nextEnableTime" column="next_enable_time"/>
        <result property="expireTime" column="expire_time"/>
        <result property="loginIp" column="login_ip"/>
        <result property="deviceCode" column="device_code"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="remark" column="remark"/>
        <result property="delFlag" column="del_flag"/>
        <association property="app" column="app_id" javaType="SysApp" resultMap="SysAppResult"/>
    </resultMap>

    <resultMap type="SysApp" id="SysAppResult">
        <result property="appId" column="app_id"/>
        <result property="appName" column="app_name"/>
        <result property="authType" column="auth_type"/>
        <result property="billType" column="bill_type"/>
        <result property="description" column="description"/>
    </resultMap>

    <sql id="selectSysAppTrialUserVo">
        select atu.app_trial_user_id,
               atu.app_id,
               atu.status,
               atu.last_login_time,
               atu.login_times,
               atu.login_times_all,
               atu.next_enable_time,
               atu.expire_time,
               atu.login_ip,
               atu.device_code,
               atu.create_by,
               atu.create_time,
               atu.update_by,
               atu.update_time,
               atu.remark,
               atu.del_flag,
               a.app_id,
               a.app_name,
               a.auth_type,
               a.bill_type,
               a.description
        from sys_app_trial_user atu
                 left join sys_app a on atu.app_id = a.app_id
    </sql>

    <select id="selectSysAppTrialUserList" parameterType="SysAppTrialUser" resultMap="SysAppTrialUserResult">
        <include refid="selectSysAppTrialUserVo"/>
        where atu.del_flag = '0'
        <if test="appId != null ">and atu.app_id = #{appId}</if>
        <if test="status != null  and status != ''">and atu.status = #{status}</if>
        <if test="params.beginLastLoginTime != null and params.beginLastLoginTime != '' and params.endLastLoginTime != null and params.endLastLoginTime != ''">
            and atu.last_login_time between #{params.beginLastLoginTime} and #{params.endLastLoginTime}
        </if>
        <if test="params.beginLoginTimes != null and params.beginLoginTimes != '' and params.endLoginTimes != null and params.endLoginTimes != ''">
            and atu.login_times between #{params.beginLoginTimes} and #{params.endLoginTimes}
        </if>
        <if test="params.beginLoginTimesAll != null and params.beginLoginTimesAll != '' and params.endLoginTimesAll != null and params.endLoginTimesAll != ''">
            and atu.login_times_all between #{params.beginLoginTimesAll} and #{params.endLoginTimesAll}
        </if>
        <if test="params.beginNextEnableTime != null and params.beginNextEnableTime != '' and params.endNextEnableTime != null and params.endNextEnableTime != ''">
            and atu.next_enable_time between #{params.beginNextEnableTime} and #{params.endNextEnableTime}
        </if>
        <if test="params.beginExpireTime != null and params.beginExpireTime != '' and params.endExpireTime != null and params.endExpireTime != ''">
            and atu.expire_time between #{params.beginExpireTime} and #{params.endExpireTime}
        </if>
        <if test="loginIp != null  and loginIp != ''">and atu.login_ip = #{loginIp}</if>
        <if test="deviceCode != null  and deviceCode != ''">and atu.device_code = #{deviceCode}</if>
        <if test="remark != null and remark != ''">and atu.remark like concat('%', #{remark}, '%')</if>
    </select>

    <select id="selectSysAppTrialUserByAppTrialUserId" parameterType="Long" resultMap="SysAppTrialUserResult">
        <include refid="selectSysAppTrialUserVo"/>
        where atu.app_trial_user_id = #{appTrialUserId}
    </select>

    <select id="selectSysAppTrialUserByLoginIp" parameterType="String" resultMap="SysAppTrialUserResult">
        <include refid="selectSysAppTrialUserVo"/>
        where atu.del_flag = '0' and atu.login_ip = #{loginIp}
    </select>

    <select id="selectSysAppTrialUserByAppIdAndLoginIpAndDeviceCode" resultMap="SysAppTrialUserResult">
        <include refid="selectSysAppTrialUserVo"/>
        where atu.del_flag = '0' and atu.app_id = #{appId} and atu.login_ip = #{loginIp} and atu.device_code =
        #{deviceCode}
    </select>

    <insert id="insertSysAppTrialUser" parameterType="SysAppTrialUser" useGeneratedKeys="true"
            keyProperty="appTrialUserId">
        insert into sys_app_trial_user
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="appId != null">app_id,</if>
            <if test="status != null and status != ''">status,</if>
            <if test="lastLoginTime != null">last_login_time,</if>
            <if test="loginTimes != null">login_times,</if>
            <if test="loginTimesAll != null">login_times_all,</if>
            <if test="nextEnableTime != null">next_enable_time,</if>
            <if test="expireTime != null">expire_time,</if>
            <if test="loginIp != null and loginIp != ''">login_ip,</if>
            <if test="deviceCode != null">device_code,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
            <if test="delFlag != null and delFlag != ''">del_flag,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="appId != null">#{appId},</if>
            <if test="status != null and status != ''">#{status},</if>
            <if test="lastLoginTime != null">#{lastLoginTime},</if>
            <if test="loginTimes != null">#{loginTimes},</if>
            <if test="loginTimesAll != null">#{loginTimesAll},</if>
            <if test="nextEnableTime != null">#{nextEnableTime},</if>
            <if test="expireTime != null">#{expireTime},</if>
            <if test="loginIp != null and loginIp != ''">#{loginIp},</if>
            <if test="deviceCode != null">#{deviceCode},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
            <if test="delFlag != null and delFlag != ''">#{delFlag},</if>
        </trim>
    </insert>

    <update id="updateSysAppTrialUser" parameterType="SysAppTrialUser">
        update sys_app_trial_user
        <trim prefix="SET" suffixOverrides=",">
            <if test="appId != null">app_id = #{appId},</if>
            <if test="status != null and status != ''">status = #{status},</if>
            <if test="lastLoginTime != null">last_login_time = #{lastLoginTime},</if>
            <if test="loginTimes != null">login_times = #{loginTimes},</if>
            <if test="loginTimesAll != null">login_times_all = #{loginTimesAll},</if>
            <if test="nextEnableTime != null">next_enable_time = #{nextEnableTime},</if>
            <if test="expireTime != null">expire_time = #{expireTime},</if>
            <if test="loginIp != null and loginIp != ''">login_ip = #{loginIp},</if>
            <if test="deviceCode != null">device_code = #{deviceCode},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="delFlag != null and delFlag != ''">del_flag = #{delFlag},</if>
        </trim>
        where app_trial_user_id = #{appTrialUserId}
    </update>

    <!--    <delete id="deleteSysAppTrialUserByAppTrialUserId" parameterType="Long">-->
    <!--        delete from sys_app_trial_user where app_trial_user_id = #{appTrialUserId}-->
    <!--    </delete>-->

    <!--    <delete id="deleteSysAppTrialUserByAppTrialUserIds" parameterType="String">-->
    <!--        delete from sys_app_trial_user where app_trial_user_id in -->
    <!--        <foreach item="appTrialUserId" collection="array" open="(" separator="," close=")">-->
    <!--            #{appTrialUserId}-->
    <!--        </foreach>-->
    <!--    </delete>-->

    <delete id="deleteSysAppTrialUserByAppTrialUserId" parameterType="Long">
        update sys_app_trial_user
        set del_flag = '2'
        where app_trial_user_id = #{appTrialUserId}
    </delete>

    <delete id="deleteSysAppTrialUserByAppTrialUserIds" parameterType="String">
        update sys_app_trial_user set del_flag = '2' where app_trial_user_id in
        <foreach collection="array" item="appTrialUserId" open="(" separator="," close=")">
            #{appTrialUserId}
        </foreach>
    </delete>
</mapper>